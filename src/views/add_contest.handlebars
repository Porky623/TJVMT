<div class="container-fluid">
    <h1>Add a Contest</h1>
    <form name="add_contest_form_test" action="{{prefix}}contest/add" method="post">
        <label for="name">Contest name</label>
        <input class="form-control" type="text" required id="name" name="name" autofocus="autofocus">
        <input class="btn btn-primary" type="submit" value="Submit">
    </form>
    <a href="{{prefix}}officers"><button type="button" class="btn btn-secondary">Return to Officers Page</button></a>
</div>
